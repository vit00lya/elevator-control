#include "xserial.hpp"
#include <iostream>
#include <string>

int main(){
    
  xserial::ComPort com = xserial::ComPort(0,
					  9600,
					  xserial::ComPort::eParity::COM_PORT_NOPARITY,
					  8,
					  xserial::ComPort::eStopBit::COM_PORT_ONESTOPBIT,
					  5,
					  "ttyACM");
  std::string in_str = com.getLine();
  std::cout << in_str << std::endl;
  
  return 0;
}
